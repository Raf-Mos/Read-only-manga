# Environment variables for different hosting platforms
# Set REACT_APP_HOSTING_PLATFORM to 'vercel' for Vercel deployment
# Set REACT_APP_HOSTING_PLATFORM to 'hostinger' for Hostinger deployment
REACT_APP_HOSTING_PLATFORM=hostinger

# For Vercel deployment, set this to true
REACT_APP_VERCEL=false

# For Hostinger deployment, set this to true  
REACT_APP_HOSTINGER=true

# Custom API proxy URL (if you have your own CORS proxy)
# REACT_APP_CUSTOM_API_PROXY=https://your-proxy.com/api/

# MangaDx API base URL (usually should not be changed)
REACT_APP_MANGADX_API_URL=https://api.mangadx.org